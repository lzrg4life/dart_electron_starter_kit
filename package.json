{
  "name": "app",
  "version": "1.0.0",
  "description": "Build Electron apps using Dart.",
  "main": "dev_entry.js",
  "scripts": {
    "build": "pub run build_runner build --config=release -o build",
    "build_dev": "pub run build_runner build",
    "build_runner_serve": "pub run build_runner serve --hot-reload",
    "dev": "run-s \"build_dev\" \"serve\"",
    "serve": "run-p \"build_runner_serve\" \"start\"",
    "start": "electron ."
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "electron": "^1.7.10"
  }
}
